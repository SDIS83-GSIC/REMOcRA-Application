    "postcss-preset-env": "10.6.1",
    "prettier": "3.8.1",
    "process": "^0.11.10",
    "stylelint": "^16.26.1",
    "stylelint-config-css-modules": "^4.6.0",
    "stylelint-config-recommended": "^14.0.1",
    "stylelint-no-unsupported-browser-features": "^8.0.5",
