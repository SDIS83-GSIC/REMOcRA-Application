{
  "name": "remocra",
  "version": "0.0.0",
  "license": "UNLICENSED",
  "private": true,
  "scripts": {
    "build": "parcel build --no-autoinstall --dist-dir build/parceljs --cache-dir build/cache/parcel src/index.html",
    "watch": "parcel watch --no-autoinstall --dist-dir build/parceljs --cache-dir build/cache/parcel src/index.html",
    "lint:js": "eslint --cache --cache-location build/cache/eslint 'src/**/*.tsx'",
    "lint:css": "stylelint --cache --cache-location build/cache/stylelint 'src/**/*.css'",
    "lint:html": "prettier --list-different 'src/**/*.html'",
    "lint": "run-p lint:*",
    "format": "prettier --write '.*rc{,.tsx}' '!.browserslistrc' '*.js' 'src/**/*.{ts,tsx,js,css,html}'",
    "bom": "npm sbom --sbom-format cyclonedx --sbom-type application --output build/reports/bom-js.json"
  },
  "dependencies": {
    "@dnd-kit/accessibility": "^3.1.0",
    "@dnd-kit/core": "^6.1.0",
    "@dnd-kit/sortable": "^8.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@flaticon/flaticon-uicons": "^3.3.1",
    "@popperjs/core": "^2.11.8",
    "@react-pdf-viewer/core": "^3.12.0",
    "@react-pdf-viewer/default-layout": "^3.12.0",
    "@react-pdf-viewer/toolbar": "^3.12.0",
    "bootstrap": "^5.3.3",
    "ci": "^2.3.0",
    "classnames": "^2.5.1",
    "cookie": "^0.6.0",
    "core-js": "^3.38.1",
    "date-fns": "^3.6.0",
    "formik": "^2.4.6",
    "lodash.debounce": "^4.0.8",
    "ol": "^10.1.0",
    "proj4": "^2.11.0",
    "prop-types": "^15.8.1",
    "react": "^18.3.1",
    "react-async": "10.0.1",
    "react-bootstrap": "^2.10.4",
    "react-bootstrap-typeahead": "^6.3.2",
    "react-dom": "^18.3.1",
    "react-pdf": "^7.7.3",
    "react-router-dom": "^6.26.2",
    "react-select": "^5.8.0",
    "regenerator-runtime": "^0.14.1",
    "use-debounce": "^10.0.3",
    "yup": "^1.4.0"
  },
  "devDependencies": {
