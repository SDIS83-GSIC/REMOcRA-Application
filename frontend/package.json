{
  "name": "remocra",
  "version": "0.0.0",
  "license": "UNLICENSED",
  "private": true,
  "scripts": {
    "build": "parcel build --no-autoinstall --dist-dir build/parceljs --cache-dir build/cache/parcel src/index.html",
    "watch": "parcel watch --no-autoinstall --dist-dir build/parceljs --cache-dir build/cache/parcel src/index.html",
    "lint:js": "eslint --cache --cache-location build/cache/eslint 'src/**/*.tsx'",
    "lint:css": "stylelint --cache --cache-location build/cache/stylelint 'src/**/*.css'",
    "lint:html": "prettier --list-different 'src/**/*.html'",
    "lint": "run-p lint:*",
    "format": "prettier --write '.*rc{,.tsx}' '!.browserslistrc' '*.js' 'src/**/*.{ts,tsx,js,css,html}'",
    "bom": "npm sbom --sbom-format cyclonedx --sbom-type application --output build/reports/bom-js.json"
  },
  "dependencies": {
    "@popperjs/core": "^2.11.8",
    "bootstrap": "^5.3.3",
    "@flaticon/flaticon-uicons": "^3.3.1",
    "ci": "^2.3.0",
    "classnames": "^2.5.1",
    "cookie": "^0.6.0",
    "core-js": "^3.38.1",
    "date-fns": "^3.6.0",
    "formik": "^2.4.6",
    "lodash.debounce": "^4.0.8",
    "prop-types": "^15.8.1",
    "react": "^18.3.1",
    "react-async": "10.0.1",
    "react-bootstrap": "^2.10.4",
    "react-bootstrap-typeahead": "^6.3.2",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.26.1",
    "react-select": "^5.8.0",
    "regenerator-runtime": "^0.14.1",
    "use-debounce": "^5.2.1",
    "react-pdf": "^7.7.3",
    "@react-pdf-viewer/core": "^3.12.0",
    "@react-pdf-viewer/default-layout": "^3.12.0",
    "@react-pdf-viewer/toolbar": "^3.12.0",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.25.4",
    "@babel/preset-react": "^7.24.7",
    "@babel/runtime": "^7.25.4",
    "@types/react": "^18.3.4",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "7.18.0",
    "@typescript-eslint/parser": "7.18.0",
    "autoprefixer": "^10.4.20",
    "babel-loader": "^9.1.3",
    "babel-plugin-inline-react-svg": "^2.0.2",
    "buffer": "^6.0.3",
    "eslint": "8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-compat": "^4.2.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.35.0",
    "eslint-plugin-react-dom": "1.12.3",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-security": "^2.1.1",
    "eslint-plugin-storybook": "0.8.0",
    "mini-css-extract-plugin": "^2.9.1",
    "npm-run-all": "^4.1.5",
    "parcel": "^2.12.0",
    "postcss": "^8.4.41",
    "postcss-import": "15.1.0",
    "postcss-modules": "4.3.1",
    "postcss-modules-values": "4.0.0",
    "postcss-preset-env": "9.6.0",
    "prettier": "3.3.3",
    "process": "^0.11.10",
