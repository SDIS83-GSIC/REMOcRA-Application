{
  "name": "remocra",
  "version": "0.0.0",
  "license": "UNLICENSED",
  "private": true,
  "scripts": {
    "build": "parcel build --no-autoinstall --dist-dir build/parceljs --cache-dir build/cache/parcel src/index.html",
    "watch": "parcel watch --no-autoinstall --dist-dir build/parceljs --cache-dir build/cache/parcel src/index.html",
    "lint:js": "eslint --cache --cache-location build/cache/eslint 'src/**/*.tsx'",
    "lint:css": "stylelint --cache --cache-location build/cache/stylelint 'src/**/*.css'",
    "lint:html": "prettier --list-different 'src/**/*.html'",
    "lint": "run-p lint:*",
    "format": "prettier --write '.*rc{,.tsx}' '!.browserslistrc' '*.js' 'src/**/*.{ts,tsx,js,css,html}'",
    "bom": "npm sbom --sbom-format cyclonedx --sbom-type application --output build/reports/bom-js.json"
  },
  "dependencies": {
    "@popperjs/core": "^2.11.8",
    "bootstrap": "^5.3.3",
    "ci": "^2.3.0",
    "classnames": "^2.5.1",
    "cookie": "^0.6.0",
    "core-js": "^3.35.1",
    "lodash.debounce": "^4.0.8",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-async": "10.0.1",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.21.3",
    "regenerator-runtime": "^0.13.9",
    "use-debounce": "^5.2.0",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@babel/core": "^7.23.7",
    "@babel/preset-env": "^7.23.8",
    "@babel/preset-react": "^7.23.3",
    "@babel/runtime": "^7.22.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@typescript-eslint/eslint-plugin": "7.0.2",
    "@typescript-eslint/parser": "7.0.2",
    "autoprefixer": "^10.4.18",
    "babel-loader": "^9.1.3",
    "babel-plugin-inline-react-svg": "^2.0.2",
    "eslint": "8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-compat": "^4.2.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-dom": "1.5.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-security": "^2.1.0",
    "eslint-plugin-storybook": "0.6.15",
    "mini-css-extract-plugin": "^2.8.1",
    "npm-run-all": "^4.1.5",
    "parcel": "^2.11.0",
    "postcss": "^8.4.33",
    "postcss-import": "15.1.0",
    "postcss-modules": "4.3.0",
    "postcss-modules-values": "4.0.0",
    "postcss-preset-env": "9.1.3",
    "prettier": "3.2.4",
    "process": "^0.11.10",
    "stylelint": "^16.2.0",
    "stylelint-config-css-modules": "^4.4.0",
    "stylelint-config-recommended": "^14.0.0",
    "stylelint-no-unsupported-browser-features": "^8.0.1",
    "stylelint-prettier": "^5.0.0",
    "typescript": "5.3.3",
    "webpack-cli": "^5.1.4"
  }
}
