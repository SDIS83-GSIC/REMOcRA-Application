{
  "name": "remocra",
  "version": "0.0.0",
  "license": "UNLICENSED",
  "private": true,
  "scripts": {
    "cleanParcelBuild": "rm -rf build/parceljs",
    "prebuild": "npm run cleanParcelBuild",
    "build": "parcel build --no-autoinstall --dist-dir build/parceljs --cache-dir build/cache/parcel src/index.html src/style.scss",
    "watch": "parcel watch --no-autoinstall --dist-dir build/parceljs --cache-dir build/cache/parcel src/index.html",
    "lint:js": "eslint --cache --cache-location build/cache/eslint 'src/**/*.tsx' --max-warnings=21",
    "lint:css": "stylelint --cache --cache-location build/cache/stylelint 'src/**/*.{css,scss}'",
    "lint:html": "prettier --list-different 'src/**/*.html'",
    "lint": "run-p lint:*",
    "format": "prettier --write '.*rc{,.tsx}' '!.browserslistrc' '*.js' 'src/**/*.{ts,tsx,js,css,html,scss}'",
    "bom": "NODE_ENV=production npx @cyclonedx/cyclonedx-npm --output-file build/reports/bom-js.json"
  },
  "dependencies": {
    "@dnd-kit/accessibility": "^3.1.1",
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@flaticon/flaticon-uicons": "^3.3.1",
    "@popperjs/core": "^2.11.8",
    "@react-pdf-viewer/core": "^3.12.0",
    "@react-pdf-viewer/default-layout": "^3.12.0",
    "@react-pdf-viewer/toolbar": "^3.12.0",
    "@types/react-csv": "^1.1.10",
    "bootstrap": "^5.3.8",
    "ci": "^2.3.0",
    "classnames": "^2.5.1",
    "cookie": "^1.1.1",
    "core-js": "^3.48.0",
    "date-fns": "^4.1.0",
    "formik": "^2.4.9",
    "lodash.debounce": "^4.0.8",
    "ol": "^10.7.0",
    "pdfjs-dist": "^3.11.174",
    "proj4": "^2.20.2",
    "prop-types": "^15.8.1",
    "react": "^18.3.1",
    "react-async": "10.0.1",
    "react-bootstrap": "^2.10.10",
    "react-bootstrap-typeahead": "^6.4.1",
    "react-csv": "^2.2.2",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.30.3",
    "react-select": "^5.10.2",
    "recharts": "^2.15.4",
    "regenerator-runtime": "^0.14.1",
    "sass": "1.97.3",
    "use-debounce": "^10.1.0",
    "yup": "^1.7.1"
  },
  "devDependencies": {
    "@babel/core": "^7.28.6",
    "@babel/preset-env": "^7.28.6",
    "@babel/preset-react": "^7.28.5",
    "@babel/runtime": "^7.28.6",
    "@cyclonedx/cyclonedx-npm": "^3.1.0",
    "@parcel/transformer-sass": "^2.16.3",
    "@types/proj4": "^2.19.0",
    "@types/react": "^18.3.27",
    "@types/react-dom": "^18.3.7",
    "@typescript-eslint/eslint-plugin": "7.18.0",
    "@typescript-eslint/parser": "7.18.0",
    "autoprefixer": "^10.4.23",
    "babel-loader": "^9.2.1",
    "babel-plugin-inline-react-svg": "^2.0.2",
    "buffer": "^6.0.3",
    "eslint": "8.57.1",
    "eslint-config-prettier": "^9.1.2",
    "eslint-plugin-compat": "^6.1.0",
    "eslint-plugin-import": "^2.32.0",
